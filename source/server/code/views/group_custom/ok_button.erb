<script>
'use strict';
$(() => {

  $('button#ok').click(() => {
    const displayName = cd.selectedDisplayName;
    const params = `display_names[]=${encodeURIComponent(displayName)}`;
    window.location = `<%= @next_url %>?${params}`;
  });

});
</script>

<div>
  <button type="button" id="ok">ok</button>
</div>
